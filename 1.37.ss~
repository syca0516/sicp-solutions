(define (cont-frac n d k)
  (define (iter n d start)
    (if (> start k)
	0
	(/ (n start)
	   (+ (d start)
	      (iter n d (+ start 1))))))
  (iter n d 1))

(define (test n d from to)
  (define (iter k)
    (if (< k to)
	(begin
	  (cont-frac n d k)
	  (iter (+ k 1)))))
  (iter from))

(test (lambda (i) 1.0)
      (lambda (i) 1.0)
      1
      10)
